{{#extend "layout"}}
{{#content "main"}}

<section class="intro intro--aspect-ratio">
    <div class="container">
        <div class="intro__content">
            {{> header }}

            <div class="intro__content-grid intro__content-grid--full-grow">
                <div class="intro__content-aside">
                    <div class="intro__breadcrumbs">
                        <a href="#">
                            Главная
                        </a>

                        <a href="#" class="active">
                            Проекты
                        </a>

                        <a href="#" class="active">
                            ЖК «Дубовый парк»
                        </a>
                    </div>

                    <div class="intro__carousel-buttons carousel-buttons carousel-buttons--white">
                        <button class="carousel-buttons__button carousel-buttons__button--left solutions-swiper-left-button">
                            <svg width="14" height="14" aria-hidden="true" class="icon">
                                <use xlink:href="#slider-arrow-new"></use>
                            </svg>
                        </button>
                        <button class="carousel-buttons__button carousel-buttons__button--right solutions-swiper-right-button">
                            <svg width="14" height="14" aria-hidden="true" class="icon">
                                <use xlink:href="#slider-arrow-new"></use>
                            </svg>
                        </button>
                    </div>
                </div>

                <div class="intro__content-inner">
                    <div class="intro__home-card intro__home-card--full">
                        <div class="intro__home-card-features">
                            <div class="intro__home-card-features-item intro-features-item">
                                <span class="intro-features-item__number">
                                    7
                                </span>
                                <p class="intro-features-item__text">
                                    минут<br> до&nbsp;«Сосновой рощи»
                                </p>
                            </div>
                            <div class="intro__home-card-features-item intro-features-item">
                                <span class="intro-features-item__number">
                                    6
                                </span>
                                <p class="intro-features-item__text">
                                    кварталов с&nbsp;закрытыми дворами
                                </p>
                            </div>
                        </div>

                        <div class="intro__home-heading-wrapper">
                            <h1 class="intro__home-card-heading">
                                Жилой Комплекс<br> «Дубовый парк»
                            </h1>

                            <ul class="intro__home-card-tabs">
                                <li class="intro__home-card-tab intro-tab online-tip">
                                    <span class="intro-tab__text">Русагрострой</span>
                                </li>
                                <li class="intro__home-card-tab intro-tab online-tip">
                                    <svg aria-hidden="true" class="icon">
                                        <use xlink:href="#tab-icon"></use>
                                    </svg>
                                    <span class="intro-tab__text">Удобные планировки</span>
                                </li>
                                <li class="intro__home-card-tab intro-tab online-tip">
                                    <span class="intro-tab__text">Срок сдачи 2024-2027</span>
                                </li>
                                <li class="intro__home-card-tab intro-tab online-tip">
                                    <span class="intro-tab__text">Есть сданные дома</span>
                                </li>
                            </ul>
                        </div>

                        <a href="#" class="btn btn--red-accent intro__home-card-link">
                            Выбрать квартиру
                        </a>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="intro__image-carousel">
        <div class="intro__image-swiper swiper">
            <ul class="intro__image-swiper-wrapper swiper-wrapper">
                <li class="intro__image-swiper-slide swiper-slide">
                    <picture>
